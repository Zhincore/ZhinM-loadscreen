{
  "name": "zhin-loadscreen",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Zhincore",
  "private": true,
  "scripts": {
    "dev": "vite",
    "watch": "concurrently \"npm:watch:nui\" \"npm:watch:fx\"",
    "watch:nui": "vite build --watch",
    "watch:fx": "node build.js",
    "build": "concurrently \"npm:build:nui\" \"npm:build:fx\"",
    "build:nui": "vite build",
    "build:fx": "node build.js --mode=production"
  },
  "devDependencies": {
    "@citizenfx/client": "^2.0.5865-1",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "@types/node": "^16.3.1",
    "concurrently": "^7.4.0",
    "esbuild": "^0.14.10",
    "fivem-router": "https://github.com/TheIndra55/fivem-router",
    "sass": "^1.54.8",
    "svelte": "^3.50.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vite-plugin-singlefile": "^0.11.1",
    "vite-tsconfig-paths": "^3.5.0",
    "yaml": "^2.1.1"
  },
  "dependencies": {}
}
